{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "max-line-length": [true, 180],
        "completed-docs": true,
        "import-spacing": true,
        "ordered-imports": true
    },
    "rulesDirectory": []
}
